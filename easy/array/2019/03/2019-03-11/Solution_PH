class Solution{
  public int removeDuplicates(int[] nums)
  {
   int j =0;
   if(nums.length == 0)
     return 0;
   if(nums.length == 1)
    return 1;
   for(int i=0;i<nums.length-1;i++)
     {
       if(nums[i]<nums[i+1])
        {
         j++;
         num[j]= num[i+1];
        }
      }
      j++;
      return j;
    }
 }
