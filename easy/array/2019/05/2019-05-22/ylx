class Solution:
    def sortArrayByParity(self, A: List[int]) -> List[int]:
        if len(A) == 1:
            return A
        l = 0
        r = len(A) - 1
        while l < r:
            if A[l] % 2 == 0:
                l += 1
                continue
            if A[r] % 2 == 1:
                r -= 1
                continue
            if A[l] % 2 == 1 and A[r] % 2 == 0:
                A[l], A[r] = A[r], A[l]
                l += 1
                r -= 1
        return A
