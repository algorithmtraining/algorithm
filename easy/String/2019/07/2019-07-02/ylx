class Solution:
    def gcdOfStrings(self, str1: str, str2: str) -> str:

        def gcd(a, b):
            if b == 0:
                return a
            else:
                return gcd(b, a % b)

        if str1 + str2 != str2 + str1:
            return ""
        return str1[0:gcd(len(str1), len(str2))]

if __name__ == '__main__':
    y = Solution()
    str1 = "ABABAB"
    str2 = "ABAB"
    print(y.gcdOfStrings(str1, str2))
